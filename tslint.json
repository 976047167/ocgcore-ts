{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-bitwise":false,
        "no-console":false,
        "variable-name":{
            "options":["allow-leading-underscore",
                "ban-keywords",
                "allow-snake-case"
            ]
        },
        "max-classes-per-file" : true,
        "object-literal-sort-keys":false,
        "no-empty":false
    },
    "rulesDirectory": []
}